				"saludation", "firstname", "second_name", "lastname",
				"social_security", "work", "gender", "language", "migratory",
				"jail", "income", "document", "relationship", "expiration", "smoke"
			]
		},
		"communication_channels": {
			"label": "Communication Channels",
			"dynamic": false,
			"fields": [
				"donotcall", "emailoptout", "homephone",
				"otherphone", "mobile", "phone", "email", "secondaryemail",
				"fax", "assistantphone"
			]
		},
			"account_address": {
			"label": "Account Address",
			"dynamic": false,
			"fields": ["ship_country", "bill_country"]
		},
		"dependents_dynamic": {
			"label": "Dependent #",
