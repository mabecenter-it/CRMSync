		},
		"Accounts": {
			"blocks": ["account_address"]
		},
		"SalesOrder": {
			"blocks": ["dependents_dynamic"]
		}
	},
	"blocks": {
		"contact_info": {
			"label": "LBL_CONTACT_INFORMATION",
